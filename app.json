{
  "expo": {
    "name": "S2 Mangás",
    "slug": "s2mangas",
    "version": "1.2.0",
    "scheme": "s2mangas",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#171717"
      },
      "package": "com.s2mangas.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a41ae741-b6cd-4ab4-a2d2-f4b1cc583238"
      }
    },
    "plugins": [
      [ "expo-font" ],
      [ "expo-av", { "microphonePermission": "Permita o Até que ele venha a acessar o microfone."  } ],
      [ "expo-build-properties" ],
      [ "expo-dev-launcher", { "launchModeExperimental": "most-recent"  } ],
    ]
  }
}
